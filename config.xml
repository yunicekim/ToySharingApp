<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.toyssharingcordovaapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Toys Sharing Cordova App</name>
    <description>
        Toys Sharing Cordova App
    </description>
    <author email="ykim0406@conestogac.on.ca" href="http://conestogac.on.ca">
       Abdel Olayyan, Yunice Kim
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>

    <icon src="icon.png" />
    <splash src="icon.png" />

    <engine name="android" spec="~7.1.4" />
    <plugin name="cordova-plugin-camera" spec="^4.0.3" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
</widget>
